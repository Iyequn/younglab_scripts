{
	"global_software_specs": {
		"use_global_specs": 1,
		"software": "spm",
		"spm_version": 12,
		"afni_version": 16
		},
	
	"node_flags": {
		"dicom": 0,
		"slicetime": 0,
		"realign": 0,
		"reslice": 0,
		"normalize": 0,
		"skull_strip" : 0,
		"smooth": 0,
		"model": 0,
		"model_reml": 0,
		"specify_design": 0,
		"design": 0,
		"onesample_T": 0,
		"twosample_T": 0,
		"estimate": 0,
		"contrast": 0,
		"cluster_correct": 0
		},

	"directories": {
		"workflow_name": "MYWORKFLOW",
		"root": "/home/younglw/lab",
		"study": "STUDY",
		"behavioral_subdir": "behavioural",
		"dicom_subdir": "dicom"
		},

	"experiment_details": {
		"subject_tag": "SUBJTAG",
		"subject_nums": ["SUBJNUMS"],
		"task_names": ["TASKNAMES"],
		"ips": {
			},
		"runs": {
			},
		"contrast_info": {
			},
		"spm_inputs": {
			},
		"design": {
			},
		"covariates": {
			}
		},

	"iterate": {
		"node_names": ["specify_design","smooth"],
		"iterate_over": [["task_name","in_files"],["fwhm"]],
		"iterate_values": [["/tom*/f*.nii","/SHAPES*/f*.nii"],[0,8]]
		},

	"params": {
		"dicom": {
			"local_software_spec": "",
			"specify_inputs": 0,
			"infile_dir": "*",
			"in_files": [],
			"output_dir_struct": "series"
			},
		"slicetime": {
			"local_software_spec": "",
			"infile_dir": "dicom*",
			"in_files": [],
			"num_slices": 36,
			"ref_slice": 1,
			"TR": 2.0
			},
		"realign": {
			"local_software_spec": "",
			"infile_dir": "slicetime*",
			"in_files": [],
			"fwhm": 5,
			"quality": 0.9,
			"register_to_mean": 0
			},
		"reslice": {
			"local_software_spec": "",
			"infile_dir": "realign*",
			"in_file": [],
			"interp": 4
			},
		"normalize": {
			"local_software_spec": "",
			"infile_dir": "reslice*",
			"source": [],
			"template": "/usr/public/spm/spm8/templates/EPI.nii"
			},
		"skull_strip": {
			"local_software_spec" : "fsl",
			"infile_dir": "",
			"frac": 0.8,
			"out_file": "skull_strip_mask"
			},
		"smooth": {
			"local_software_spec": "",
			"infile_dir": "normalize*",
			"in_files": [],
			"fwhm": [5, 5, 5]
			},
		"specify_design": {
			"local_software_spec": "",
			"infile_dir": "smooth*",
			"functional_runs": [],
			"subject_info": [],
			"high_pass_filter_cutoff": 128,
			"input_units": "scans",
			"time_repetition": 2.0
			},
		"design": {
			"local_software_spec": "",
			"specify_inputs": 0,
			"infile_dir": "specify_design*",
			"mask_image_dir" : "",
			"mask_image_template" : "",
			"session_info": "",
			"bases": {
				"hrf": {
					"derivs": [0, 0]
					} 
				},
			"interscan_interval": 2.0,
			"timing_units": "scans"
			},
		"estimate": {
			"local_software_spec": "",
			"specify_inputs": 0,
			"infile_dir": "design*",
			"spm_mat_file": "",
			"estimation_method": {
				"Classical": 1
				}
			},
		"onesample_T": {
			"local_software_spec": "",
			"specify_inputs": 0,
			"infile_dir": "estimate*",
			"in_files": [],
			"threshold_mask_none": true,
			"global_calc_omit": true,
			"no_grand_mean_scaling": true,
			"use_implicit_threshold": true
			},
		"twosample_T": {
			"local_software_spec": "",
			"specify_inputs": 0,
			"infile_dir_1": "estimate*",
			"infile_dir_2": "estimate*",
			"infile_template_1" : "*nii",
			"infile_template_2" : "*nii",
			"threshold_mask_none": true,
			"global_calc_omit": true,
			"no_grand_mean_scaling": true,
			"use_implicit_threshold": true,
			"unequal_variance": true
			},
		"contrast": {
			"local_software_spec": "",
			"specify_inputs": 0,
			"infile_dir": "estimate*",
			"contrasts": {
				"1": {
					"name": "A > B",
					"cond_names": ["A","B"],
					"con_vals": [1,-1]
					}
				},
			"contrast_type": "T",
			"Fcontrast_name": ""
			},
		"cluster_correct": {
			"local_software_spec": "",
			"specify_inputs": 0,
			"infile_dir": "contrast*",
			"template": "spmT*.nii",
			"contrast_index": 1,
			"cluster_p_thresh": 0.05,
			"cluster_k_extent": 10,
			"voxel_p_thresh": 0.001,
			"use_fwe_correction": true
			}

		}
}